<h1>Listing <%= @count %> Dojos</h1>

<table>
    <tr>
        <th>Coding Dojo Branch</th>
        <th>Street</th>
        <th>Ciy</th>
        <th>State</th>
        <th>Actions</th>
    </tr>
<% @dojos.each do |dojo|%>
    <tr>
    <td><%= dojo.name %></td>
    <td><%= dojo.street %></td>
    <td><%= dojo.city %></td>
    <td><%= dojo.state %></td>
    <td><a href='/dojos/show/<%= dojo.id %>'>Show</a>
    <a href='/dojos/edit'>Edit</a>
    <a href='/dojos/destroy'>Destroy</a>
    </td>
    </tr>
<% end %>

</table>
<br>
<br>
<a href='/dojos/new'>Add Dojo!</a>
